node('java') {
    stage('Git') {
    git branch: 'Dev', url: 'https://github.com/Karthick-Gopalakrishnan/webapp.git'
   }
    stage('Build') {
	sh script: 'mvn package'
   }
    stage('Artifact'){
    archive 'target/*.war'
    }

    stage('Deploy'){
    sh 'scp *.war ansible@172.16.124.6:/home/ansible/webapp-pipline-Day-DP'
    build 'webapp-pipline-Day-DP'
    }
}


     
     

