node('java') {
    stage('Git') {
    git branch: 'Dev', url: 'https://github.com/Karthick-Gopalakrishnan/webapp.git'
   }
    stage('Build') {
	sh script: 'mvn package'
   }
    stage('Artifact'){
    archive 'target/*.war'
    }

    stage('Deploy'){
    sh 'mkdir Deliverables/$JOB_NAME/$BUILD_NUMBER'
    sh 'cp target/*.war Deliverables/$JOB_NAME/$BUILD_NUMBER'
    }
}


     
     

