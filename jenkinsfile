node('java') {
    stage('Git') {
    git branch: 'Dev', url: 'https://github.com/Karthick-Gopalakrishnan/webapp.git'
   }
    stage('Build') {
	sh script: 'mvn package'
   }
    stage('Artifact'){
    archive 'target/*.war'
    }

    stage('email'){
    emailext body: 'sucess ', subject: 'webapp', to: 'g.karthick@outlook.com'
    }
}


